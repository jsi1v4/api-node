model ProfileType {
  id Int @id @default(autoincrement())

  name    String
  Profile Profile[]
}

model Profile {
  id Int @id @default(autoincrement())

  title   String
  content String
  link String
  image String

  user   User        @relation(fields: [userId], references: [id])
  userId Int
  type   ProfileType @relation(fields: [typeId], references: [id])
  typeId Int
}
