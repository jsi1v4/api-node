model ProfileType {
  id Int @id @default(autoincrement())

  description    String

  Profile Profile[]
}

model Profile {
  id Int @id @default(autoincrement())

  title   String
  content String
  link    String
  image   String

  user   User        @relation(fields: [userid], references: [id])
  userid Int
  type   ProfileType @relation(fields: [typeid], references: [id])
  typeid Int
}
