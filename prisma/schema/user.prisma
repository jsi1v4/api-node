model User {
  id Int @id @default(autoincrement())
  name             String
  photo            String?
  birthday         DateTime?

  FinancesBuget    FinancesBuget[]
  FinancesForecast FinancesForecast[]
  Profile          Profile[]
  Login            Login[]
}

model Login {
  id Int @id @default(autoincrement())

  email    String @unique
  password String

  user   User @relation(fields: [userid], references: [id])
  userid Int
}
